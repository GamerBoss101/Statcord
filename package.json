{
  "name": "statcord",
  "version": "1.0.0",
  "description": "Discord rich Presense Program",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder build"
  },
  "author": "Sir Blob",
  "license": "ISC",
  "dependencies": {
    "colors": "^1.4.0",
    "discord-rpc": "^4.0.1",
    "discord.js": "^13.2.0",
    "find-process": "^1.4.5",
    "moment": "^2.29.1",
    "path": "^0.12.7",
    "uid-generator": "^2.0.0"
  },
  "devDependencies": {
    "electron": "^15.1.2",
    "electron-builder": "^22.13.1",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^15.4.0",
    "electron-rebuild": "^3.2.3",
    "fs": "^0.0.1-security",
    "node-fetch": "^3.0.0"
  },
  "build": {
    "appId": "ml.bosstop.statcord",
    "productName": "Statcord",
    "win": {
      "target": "nsis",
      "asar": "true"
    },
    "nsis": {
      "license": "./eula.txt",
      "deleteAppDataOnUninstall": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Statcord",
      "oneClick": false,
      "allowElevation": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true
    }
  }
}
